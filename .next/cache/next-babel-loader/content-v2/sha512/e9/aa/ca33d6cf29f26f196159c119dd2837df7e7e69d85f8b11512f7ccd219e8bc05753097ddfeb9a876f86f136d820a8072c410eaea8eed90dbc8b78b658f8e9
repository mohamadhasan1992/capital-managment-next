{"ast":null,"code":"import * as actionTypes from \"./actionTypes\";\nimport axios from \"axios\"; //handle loading spinner\n\nexport var authStart = function authStart() {\n  return {\n    type: actionTypes.AUTH_START\n  };\n}; //grab the token and user info from response\n\nexport var authSuccess = function authSuccess(authData) {\n  return {\n    type: actionTypes.AUTH_SUCCESS,\n    authData: authData\n  };\n}; //grab the error data from response\n\nexport var authFail = function authFail(error) {\n  return {\n    type: actionTypes.AUTH_FAIL,\n    error: error\n  };\n}; //run asynchronous code and get response from server\n//login asynchronous\n\nexport var auth = function auth(email, password) {\n  return function (dispatch) {\n    dispatch(authStart());\n    axios.post(\"http://127.0.0.2:8000/api/v1/user/login\", {\n      email: email,\n      password: password\n    }).then(function (response) {\n      console.log(response);\n      dispatch(authSuccess(response.data));\n    })[\"catch\"](function (err) {\n      return dispatch(authFail(err));\n    });\n  };\n}; //sign up asynchronous\n\nexport var signUp = function signUp(user) {\n  return function (dispatch) {\n    dispatch(authStart());\n    axios.post(\"http://127.0.0.2:8000/api/v1/user/signup\", {\n      username: user.username,\n      email: user.email,\n      password: user.password,\n      passwordConfirm: user.passwordConfirm\n    }).then(function (response) {\n      dispatch(authSuccess(response.data));\n    })[\"catch\"](function (err) {\n      return dispatch(authFail(err));\n    });\n  };\n};","map":{"version":3,"sources":["C:/Users/mohamadhasan tabrizi/Desktop/capitalnext/store/actions/auth.js"],"names":["actionTypes","axios","authStart","type","AUTH_START","authSuccess","authData","AUTH_SUCCESS","authFail","error","AUTH_FAIL","auth","email","password","dispatch","post","then","response","console","log","data","err","signUp","user","username","passwordConfirm"],"mappings":"AAAA,OAAO,KAAKA,WAAZ,MAA6B